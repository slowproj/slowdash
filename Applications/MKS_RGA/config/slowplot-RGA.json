{
    "control": {
        "range": { "length": 3600, "to": 0 },
        "reload": 2,
        "mode": "normal",
        "grid": { "rows": 2, "columns": 2 }
    },
    "style": { },
    "panels": [
        {
            "type": "singles",
            "item_type": "square",
            "items": [
                {
                    "channel": "Status.RGA['Status/has_control']",
                    "gauge": { "min": 0, "max": 0 },
                    "ranges": {
                        "normal": { "min": 0, "max": 0.5 },
                        "warn": { "min": 0, "max": 1.5 },
                        "error": { "min": 0, "max": 0 }
                    },
                    "label": "Device Control",
                    "text_transform": "replace(true,'Acquired')->replace(false,'Released')"
                },
                {
                    "channel": "Status.RGA['Status/filament']->replace('OFF',0)->replace('ON',1)",
                    "gauge": { "min": 0, "max": 0 },
                    "ranges": {
                        "normal": { "min": 0, "max": 0.5 },
                        "warn": { "min": 0, "max": 1.5 },
                        "error": { "min": 0, "max": 0 }
                    },
                    "label": "Filament",
                    "text_transform": "replace(0,'Off')->replace(1,'On')"
                },
                {
                    "channel": "Status.RGA['Status/multiplier']->replace('On',1)->replace('Off',0)",
                    "ranges": {
                        "normal": { "min": 0, "max": 0.5 },
                        "warn": { "min": 0, "max": 1.5 },
                        "error": { "min": 0, "max": 1000 }
                    },
                    "label": "Multiplier (auto on)",
                    "text_transform": "replace(0,'Off')->replace(1,'On')",
                    "gauge": { "min": 0, "max": 0 }
                },
                {
                    "channel": "RunControl.RGA['running']",
                    "gauge": { "min": 0, "max": 0 },
                    "ranges": {
                        "normal": { "min": 0, "max": 0.5 },
                        "warn": { "min": 0, "max": 1.5 },
                        "error": { "min": 0, "max": 0 }
                    },
                    "text_transform": "replace(true,'Running')->replace(false,'Idle')",
                    "label": "Run Status"
                },
                {
                    "channel": "Status.RGA['Settings/detector_index']",
                    "gauge": { "min": 0, "max": 3 },
                    "ranges": {
                        "normal": { "min": 0, "max": 0 },
                        "warn": { "min": 0, "max": 4 },
                        "error": { "min": 0, "max": 1000 }
                    },
                    "label": "Detector",
                    "text_transform": "replace(0,'Faraday')->replace(1,'SEM 1')->replace(2,'SEM 2)->replace(3,'SEM 3')"
                },
                {
                    "channel": "Status.RGA['Settings/accuracy']",
                    "gauge": { "min": 0, "max": 8 },
                    "ranges": {
                        "normal": { "min": 0, "max": 0 },
                        "warn": { "min": 0, "max": 8 },
                        "error": { "min": -1000, "max": 1000 }
                    },
                    "label": "Accuracy",
                    "format": "%d/8"
                },
                {
                    "channel": "Status.RGA['Status/last_mass']",
                    "gauge": { "min": 0, "max": 200 },
                    "ranges": {
                        "normal": { "min": 0, "max": 0.01 },
                        "warn": { "min": 0, "max": 200 },
                        "error": { "min": 0, "max": 0 }
                    },
                    "label": "Scan Status",
                    "text_transform": "",
                    "format": "m=%.1f"
                },
                {
                    "channel": "RunControl.RGA['time_to_next_scan']",
                    "ranges": {
                        "normal": { "min": 0, "max": 0.9 },
                        "warn": { "min": 0, "max": 864000 },
                        "error": { "min": 0, "max": 0 }
                    },
                    "label": "Next Scan",
                    "format": "in %d sec",
                    "gauge": { "min": 0, "max": 0 }
                }
            ],
            "color": { "base": "rgb(0, 0, 0)", "normal": "#62a0ea", "warn": "#ff7800", "error": "#d81b60", "value_opacity": 1, "gauge_opacity": 0.4, "tile_opacity": 0.4 },
            "grid": { "rows": 2, "columns": 4 },
            "margin": { "top": 0, "right": 0, "bottom": 0, "left": 0 },
            "ranges": { "apply_to_value": false, "apply_to_gauge": false, "apply_to_tile": true }
        },
        { "type": "html", "location": "project", "file": "RGA_control", "title": "" },
        {
            "type": "plot",
            "plots": [
                { "type": "bar", "channel": "graph.MassSpec.RGA", "color": "#009090", "label": "Mass Spectrum", "format": "%.5g", "opacity": 1 }
            ],
            "axes": { "xfixed": false, "yfixed": true, "xlog": false, "ylog": true, "zlog": false, "xmin": -0.6, "xmax": 40, "ymin": "1e-12", "ymax": 0.000001, "zmin": 0, "zmax": 1, "xtitle": "Mass Number", "ytitle": "Partial Pressure (mbar)" },
            "legend": { "style": "transparent" }
        },
        {
            "type": "timeseries",
            "plots": [
                { "type": "timeseries", "channel": "mbar.Mass02.RGA", "color": "#009090", "label": "Mass02", "format": "%.3g mbar", "opacity": 1, "marker_type": "circle", "marker_size": 2, "line_width": 1, "line_type": "connect", "fill_opacity": 0, "fill_envelope": false, "fill_baseline": "1e-100" },
                { "type": "timeseries", "channel": "mbar.Mass04.RGA", "color": "#dd8000", "label": "Mass04", "format": "%.3g mbar", "opacity": 1, "marker_type": "circle", "marker_size": 2, "line_width": 1, "line_type": "connect", "fill_opacity": 0, "fill_envelope": false, "fill_baseline": "1e-100" },
                { "type": "timeseries", "channel": "mbar.Mass16.RGA", "color": "#4169e1", "label": "Mass16", "format": "%.3g mbar", "opacity": 1, "marker_type": "circle", "marker_size": 2, "line_width": 1, "line_type": "connect", "fill_opacity": 0, "fill_envelope": false, "fill_baseline": "1e-100" },
                { "type": "timeseries", "channel": "mbar.Mass28.RGA", "color": "#b030b0", "label": "Mass28", "format": "%.3g mbar", "opacity": 1, "marker_type": "circle", "marker_size": 2, "line_width": 1, "line_type": "connect", "fill_opacity": 0, "fill_envelope": false, "fill_baseline": "1e-100" }
            ],
            "axes": { "xfixed": false, "yfixed": true, "xlog": false, "ylog": true, "zlog": false, "ymin": "1e-12", "ymax": 0.000001, "title": "Historical Partial Pressures", "ytitle": "(mbar)" },
            "legend": { "style": "side" }
        },
        { "type": "tree", "channel": "Status.RGA" },
        { "type": "tree", "channel": "RunControl.RGA" }
    ],
    "meta": { "name": "RGA", "title": "", "description": "" }
}

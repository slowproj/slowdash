{
    "control": {
        "range": { "length": 300, "to": 0 },
        "reload": 60,
        "mode": "normal",
        "grid": { "rows": 2, "columns": 2 }
    },
    "style": { },
    "panels": [
        {
            "type": "singles",
            "item_type": "square",
            "items": [
                {
                    "channel": "Status.RGA['System/controlling']->replace('Yes',true)->replace('No',false)",
                    "gauge": { "min": 0, "max": 0 },
                    "ranges": {
                        "normal": { "min": 0, "max": 0.5 },
                        "warn": { "min": 0, "max": 1.5 },
                        "error": { "min": 0, "max": 0 }
                    },
                    "label": "Device Control",
                    "text_transform": "replace(true,'Acquired')->replace(false,'Released')"
                },
                {
                    "channel": "Info.RGA['FilamentInfo/SummaryState']->replace('OFF',0)->replace('ON',1)",
                    "gauge": { "min": 0, "max": 0 },
                    "ranges": {
                        "normal": { "min": 0, "max": 0.5 },
                        "warn": { "min": 0, "max": 1.5 },
                        "error": { "min": 0, "max": 0 }
                    },
                    "label": "Filament",
                    "text_transform": "replace(0,'Off')->replace(1,'On')"
                },
                {
                    "channel": "RunStatus.RGA['running']",
                    "gauge": { "min": 0, "max": 0 },
                    "ranges": {
                        "normal": { "min": 0, "max": 0.5 },
                        "warn": { "min": 0, "max": 1.5 },
                        "error": { "min": 0, "max": 0 }
                    },
                    "text_transform": "replace(true,'Running')->replace(false,'Idling')",
                    "label": "Run Status"
                },
                {
                    "channel": "RunStatus.RGA['scanning']",
                    "gauge": { "min": 0, "max": 0 },
                    "ranges": {
                        "normal": { "min": 0, "max": 0.5 },
                        "warn": { "min": 0, "max": 1.5 },
                        "error": { "min": 0, "max": 0 }
                    },
                    "label": "Scan Status",
                    "text_transform": "replace(true,'Scanning')->replace(false,'Waiting')"
                },
                {
                    "channel": "Status.RGA['ScanSettings/detector_index']",
                    "gauge": { "min": 0, "max": 3 },
                    "ranges": {
                        "normal": { "min": 0, "max": 0.5 },
                        "warn": { "min": 0, "max": 4 },
                        "error": { "min": 0, "max": 1000 }
                    },
                    "label": "Detector Multiplier",
                    "text_transform": "replace(0,'Faraday')->replace(1,'SEM 1')->replace(2,'SEM 2)->replace(3,'SEM 3')"
                },
                {
                    "channel": "Status.RGA['ScanSettings/accuracy']",
                    "gauge": { "min": 0, "max": 8 },
                    "ranges": {
                        "normal": { "min": 0, "max": 0 },
                        "warn": { "min": 0, "max": 8 },
                        "error": { "min": -1000, "max": 1000 }
                    },
                    "label": "Accuracy",
                    "format": "%d/8"
                },
                {
                    "channel": "Status.RGA['ScanSettings/mass_end']",
                    "gauge": { "min": 0, "max": 200 },
                    "ranges": {
                        "normal": { "min": 0, "max": 0.5 },
                        "warn": { "min": 0, "max": 200 },
                        "error": { "min": 0, "max": 10000 }
                    },
                    "label": "Scan Range Max",
                    "format": "Mass %d"
                },
                {
                    "channel": "RunStatus.RGA['scan_interval']",
                    "gauge": { "min": 0, "max": 0 },
                    "ranges": {
                        "normal": { "min": 0, "max": 0.5 },
                        "warn": { "min": 0, "max": 86401 },
                        "error": { "min": -1000000, "max": 1000000 }
                    },
                    "label": "Scan Interval",
                    "format": "%.0f sec",
                    "text_transform": ""
                }
            ],
            "color": { "base": "rgb(0, 0, 0)", "normal": "#62a0ea", "warn": "#ff7800", "error": "#d81b60", "value_opacity": 1, "gauge_opacity": 0.4, "tile_opacity": 0.4 },
            "grid": { "rows": 2, "columns": 4 },
            "margin": { "top": 0, "right": 0, "bottom": 0, "left": 0 },
            "ranges": { "apply_to_value": false, "apply_to_gauge": false, "apply_to_tile": true }
        },
        { "type": "html", "location": "project", "file": "RGA_control", "title": "" },
        {
            "type": "plot",
            "plots": [
                { "type": "bar", "channel": "mass_spectrum", "color": "#009090", "label": "Mass Spectrum", "format": "%.5g", "opacity": 1 }
            ],
            "axes": { "xfixed": false, "yfixed": false, "xlog": false, "ylog": true, "zlog": false, "xmin": -0.6, "xmax": 40, "ymin": "-2e-7", "ymax": "5e-7", "zmin": 0, "zmax": 1, "xtitle": "Mass Number", "ytitle": "Partial Pressure (mbar)" },
            "legend": { "style": "none" }
        },
        { "type": "tree", "channel": "Status.RGA" },
        { "type": "tree", "channel": "RunStatus.RGA" }
    ],
    "meta": { "name": "RGA", "title": "", "description": "" }
}
